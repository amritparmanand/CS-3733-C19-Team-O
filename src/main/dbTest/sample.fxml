               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="clearButton" layoutX="177.0" layoutY="137.0" mnemonicParsing="false" onMouseClicked="#clearData" text="Clear Data" />
            <Label fx:id="field1" layoutX="15.0" layoutY="193.0" prefHeight="292.0" prefWidth="320.0" />
